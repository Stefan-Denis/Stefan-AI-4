{
    "compilerOptions": {
        "target": "es2019",
        "module": "ES2022",
        "moduleResolution": "node",
        "outDir": "../js",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./app.ts"
    ],
    "exclude": [
        "node_modules",
        "modules"
    ]
}